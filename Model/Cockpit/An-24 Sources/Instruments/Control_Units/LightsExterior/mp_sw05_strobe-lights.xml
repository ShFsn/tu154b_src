<?xml version="1.0"?>

<!-- Strobe lights OSS-61 (Bottom and V-Stab) ON/OFF; Item 327 -->
<PropertyList>
 <path>../../../shared/switches/2vg-15k.ac</path>

 <animation>
  <type>pick</type>
  <object-name>clickspot_up</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
	interpolate("an24/lights/sw_OSS-61", 1.0, 0.1 );
	sound.click("azs", 2.1, -0.21, 0.65);
      </script>
    </binding>
  </action>
   <hovered>
    <binding>
      <command>set-tooltip</command>
      <tooltip-id>OSS_on</tooltip-id>
      <label>OSS Strobe-Lights</label>
    </binding>
   </hovered>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>clickspot_down</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
	interpolate("an24/lights/sw_OSS-61", 0.0, 0.1 );
	sound.click("azs", 2.1, -0.21, 0.65);
      </script>
    </binding>
  </action>
   <hovered>
    <binding>
      <command>set-tooltip</command>
      <tooltip-id>OSS_off</tooltip-id>
      <label>OSS Strobe-Lights</label>
    </binding>
   </hovered>
 </animation>

 <animation>
   <type>rotate</type>
   <object-name>lever</object-name>
   <property>an24/lights/sw_OSS-61</property>
  <interpolation>
    <entry><ind>0.0</ind>	<dep>-20.0</dep></entry>
    <entry><ind>1.0</ind>	<dep>20.0</dep></entry>
  </interpolation>
  <axis>
    <x>0.0</x>
    <y>-1.0</y>
    <z>0.0</z>
  </axis>
 </animation>
 
</PropertyList>
