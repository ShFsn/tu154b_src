<?xml version="1.0"?>

<!-- Nosewheel steering, handle and/or pedals; anti-skid -->

<PropertyList>

<!-- Gear up/down -->

<model>
 <path>sw_gear-lever.xml</path>
  <offsets>
    <pitch-deg>-43</pitch-deg>
    <x-m>2.633</x-m>
    <y-m>0.111</y-m>
    <z-m>0.52</z-m>
  </offsets>
</model>

<model>
