  <offsets>
    <pitch-deg>43</pitch-deg>
    <x-m>2.633</x-m>
    <y-m>0.111</y-m>
    <z-m>0.52</z-m>
  </offsets>
</model>

<!-- Steering modes -->

<model>
 <path>sw_steering-mode.xml</path>
  <offsets>
    <pitch-deg>-25</pitch-deg>
    <x-m>2.125</x-m>
    <y-m>-0.86256</y-m>
    <z-m>0.638</z-m>
  </offsets>
</model>

<model>
