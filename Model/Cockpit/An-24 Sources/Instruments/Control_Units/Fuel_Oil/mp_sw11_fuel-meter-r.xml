<?xml version="1.0"?>

<!-- Fuel meter right, bottom panel switch 7 (Item 618) -->

<PropertyList>
 <path>../../../shared/switches/2pp-250.ac</path>

 <animation>
  <type>pick</type>
  <object-name>clickspot_up</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
        interpolate("an24/FuelControl/fuel-meter-r", 1.0, 0.1 );
      </script>
    </binding>
  </action>
  <hovered>
    <binding>
     <command>set-tooltip</command>
     <tooltip-id>2ppt1R_on</tooltip-id>
      <label> Right 2PPT1
Fuelmeter ON</label>
    </binding>
  </hovered>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>clickspot_down</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
        interpolate("an24/FuelControl/fuel-meter-r", 0.0, 0.1 );
      </script>
    </binding>
  </action>
  <hovered>
    <binding>
     <command>set-tooltip</command>
     <tooltip-id>2ppt1R_off</tooltip-id>
      <label>  Right 2PPT1
Fuelmeter OFF</label>
    </binding>
  </hovered>
 </animation>

 <animation>
   <type>rotate</type>
   <object-name>lever</object-name>
   <property>an24/FuelControl/fuel-meter-r</property>
  <interpolation>
    <entry><ind>0.0</ind>	<dep>-25.0</dep></entry>
    <entry><ind>1.0</ind>	<dep>25.0</dep></entry>
  </interpolation>
  <axis>
    <x>0.0</x>
    <y>-1.0</y>
    <z>0.0</z>
  </axis>
 </animation>

 <animation>
  <object-name>screws</object-name>
  <type>pick</type>
  <visible>false</visible>
 </animation>

</PropertyList>
