<?xml version="1.0"?>

<!-- Switch 2580 for heating (hot air supply to) turbogenerator TG-16 -->

<PropertyList>
 <path>../../../shared/switches/vg-15k.ac</path>

<animation>
  <type>pick</type>
  <object-name>clickspot_up</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
        interpolate("an24/PRT-24/sw_test_tanks", 1.0, 0.1 );
      </script>
    </binding>
  </action>
   <hovered>
    <binding>
      <command>set-tooltip</command>
      <tooltip-id>controlwatertankson</tooltip-id>
      <label>Control PRT-24
Water Tanks</label>
    </binding>
   </hovered>
</animation>

<animation>
  <type>pick</type>
  <object-name>clickspot_down</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
        interpolate("an24/PRT-24/sw_test_tanks", 0.0, 0.1 );
      </script>
    </binding>
  </action>
   <hovered>
    <binding>
      <command>set-tooltip</command>
      <tooltip-id>controlwatertanksoff</tooltip-id>
      <label>Control PRT-24
Water Tanks</label>
    </binding>
   </hovered>
</animation>

 <animation>
   <type>rotate</type>
   <object-name>lever</object-name>
   <property>an24/PRT-24/sw_test_tanks</property>
  <interpolation>
    <entry><ind>1.0</ind>	<dep>30.0</dep></entry>
    <entry><ind>0.0</ind>	<dep>-30.0</dep></entry>
  </interpolation>
  <axis>
    <x>0.0</x>
    <y>-1.0</y>
    <z>0.0</z>
  </axis>
 </animation>

</PropertyList>
