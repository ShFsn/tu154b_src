<?xml version="1.0"?>

<!-- Fire extinguishing and signaling main switch lock bottom right -->

<PropertyList>
 <path>../../shared/switches/switch-lock.ac</path>

 <animation>
   <type>rotate</type>
   <object-name>switch-lock</object-name>
   <property>an24/FireFeather/lock_main</property>
  <factor>90</factor>
  <center>
    <x-m>0.0</x-m>
    <y-m>0.0</y-m>
    <z-m>0.0</z-m>
  </center>
  <axis>
    <x>1.0</x>
    <y>0.0</y>
    <z>0.0</z>
  </axis>
 </animation>

</PropertyList>
