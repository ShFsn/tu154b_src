<?xml version="1.0"?>

<!-- Prepare TG-16 switch -->
<PropertyList>
 <path>../../shared/switches/good-for-all.ac</path>

 <animation>
  <type>pick</type>
  <object-name>clickspot_up</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
	interpolate("an24/Start-Panel/sw_tg16-prepare", 1.0, 0.1 );
	sound.click("azr", 2.45, 0.83, 0.5);
      </script>
    </binding>
  </action>
 </animation>

 <animation>
  <object-name>clickspot_middle</object-name>
  <type>pick</type>
  <visible>false</visible>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>clickspot_down</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
	interpolate("an24/Start-Panel/sw_tg16-prepare", 0.0, 0.1 );
	sound.click("azr", 2.45, 0.83, 0.5);
      </script>
    </binding>
  </action>
 </animation>

 <animation>
   <type>rotate</type>
   <object-name>lever</object-name>
   <property>an24/Start-Panel/sw_tg16-prepare</property>
  <interpolation>
    <entry><ind>0.0</ind>	<dep>30.0</dep></entry>
    <entry><ind>1.0</ind>	<dep>-30.0</dep></entry>
  </interpolation>
  <axis>
    <x>0.0</x>
    <y>1.0</y>
    <z>0.0</z>
  </axis>
 </animation>

</PropertyList>
