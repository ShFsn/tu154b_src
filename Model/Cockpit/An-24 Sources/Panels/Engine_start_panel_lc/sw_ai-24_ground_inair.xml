<?xml version="1.0"?>

<!-- "Ground/in air" sw -->
<PropertyList>
 <path>../../shared/switches/vg-15k.ac</path>

 <animation>
  <type>pick</type>
  <object-name>clickspot_up</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
	setprop("an24/Start-Panel/ground-air", 1.0 );
	interpolate("an24/Start-Panel/ground-air_sw", 1.0, 0.1 );
	sound.click("azr", 2.45, 0.83, 0.5);
      </script>
    </binding>
  </action>
   <hovered>
     <binding>
       <command>set-tooltip</command>
       <label>  AI-24 Engine
on Ground Start</label>
       <tooltip-id>air_ai-24</tooltip-id>
     </binding>
   </hovered>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>clickspot_down</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
	setprop("an24/Start-Panel/ground-air", 0.0 );
	interpolate("an24/Start-Panel/ground-air_sw", 0.0, 0.1 );
	sound.click("azr", 2.45, 0.83, 0.5);
      </script>
    </binding>
  </action>
   <hovered>
     <binding>
       <command>set-tooltip</command>
       <label>  AI-24 Engine
in Air Start</label>
       <tooltip-id>ground_ai-24</tooltip-id>
     </binding>
   </hovered>
 </animation>

 <animation>
   <type>rotate</type>
   <object-name>lever</object-name>
   <property>an24/Start-Panel/ground-air_sw</property>
  <interpolation>
    <entry><ind>0.0</ind>	<dep>-30.0</dep></entry>
    <entry><ind>1.0</ind>	<dep>30.0</dep></entry>
  </interpolation>
  <axis>
    <x>0.0</x>
    <y>-1.0</y>
    <z>0.0</z>
  </axis>
 </animation>

</PropertyList>
