<?xml version="1.0"?>

<!-- Left STG-18 generator CONNECT (item 708) -->
<PropertyList>
 <path>../../shared/switches/png-15k.ac</path>

 <animation>
  <type>pick</type>
  <object-name>clickspot_up</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
        interpolate("an24/Electrical_Panel/sw_stg18l", 1.0, 0.1 );
	sound.click("azr", 3.9, 0.55, 0.4);
      </script>
    </binding>
  </action>
   <hovered>
    <binding>
      <command>set-tooltip</command>
      <tooltip-id>stg18lon</tooltip-id>
      <label>    Include
Left STG-18</label>
    </binding>
   </hovered>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>clickspot_down</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
        interpolate("an24/Electrical_Panel/sw_stg18l", 0.0, 0.1 );
	sound.click("azr", 3.9, 0.55, 0.4);
      </script>
    </binding>
  </action>
   <hovered>
    <binding>
      <command>set-tooltip</command>
      <tooltip-id>stg18loff</tooltip-id>
      <label>    Exclude
Left STG-18</label>
    </binding>
   </hovered>
 </animation>

 <animation>
   <type>rotate</type>
   <object-name>lever</object-name>
   <property>an24/Electrical_Panel/sw_stg18l</property>
  <interpolation>
    <entry><ind>1.0</ind>	<dep>20.0</dep></entry>
    <entry><ind>0.0</ind>	<dep>-20.0</dep></entry>
  </interpolation>
  <axis>
    <x>0.0</x>
    <y>-1.0</y>
    <z>0.0</z>
  </axis>
 </animation>
</PropertyList>
