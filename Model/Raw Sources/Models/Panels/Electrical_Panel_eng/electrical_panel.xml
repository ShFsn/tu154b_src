<!-- GS-24/source select switches' slider -->
<animation>
   <type>slider</type>
   <object-name>slider</object-name>
   <property>an24/Electrical_Panel/slider</property>
   <factor>0.009</factor>
   <axis>
    <x>0</x>
    <y>-1</y>
    <z>0</z>
   </axis>
   <action>
    <binding>
      <command>nasal</command>
      <script>
	if ( getprop("an24/Electrical_Panel/sw_gs24") == 0.0 and getprop("an24/Electrical_Panel/sw_board-aerodrome") == 0.0 ) {
		if ( getprop("an24/Electrical_Panel/slider") == 0.0 ) {
		interpolate("an24/Electrical_Panel/slider", 1.0, 0.1 );
		}
		else {
		interpolate("an24/Electrical_Panel/slider", 0.0, 0.1 );
		}
	}
      </script>
    </binding>
   </action>
</animation>

