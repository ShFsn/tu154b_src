<?xml version="1.0"?>

<!-- "Discontinue TG-start" btn -->

<PropertyList>
 <path>../../shared/knobs_buttons/spring_load_btn01.ac</path>

<animation>
  <type>pick</type>
  <object-name>press_me</object-name>
  <visible>true</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
	enginestart.gt16_startseq.stop();
	setprop("controls/engines/engine[2]/starter", 0.0 );
	setprop("controls/engines/engine[2]/cutoff", 1.0 );
	interpolate("an24/Start-Panel/discontinuestarttg-btn", -1.0, 0.1 );
      </script>
    </binding>
   <mod-up>
    <binding>
      <command>nasal</command>
      <script>
	interpolate("an24/Start-Panel/discontinuestarttg-btn", 0.0, 0.1 );
      </script>
    </binding>
   </mod-up>
  </action>
   <hovered>
     <binding>
       <command>set-tooltip</command>
       <label>Discontinue TG-16 APU Start</label>
       <tooltip-id>disc_tg-16</tooltip-id>
     </binding>
   </hovered>
</animation>

<animation>
  <type>translate</type>
  <object-name>press_me</object-name>
  <property>an24/Start-Panel/discontinuestarttg-btn</property>
  <factor>0.003</factor>
  <axis>
    <x>0</x>
    <y>0</y>
    <z>1</z>
  </axis>
</animation>

</PropertyList>
