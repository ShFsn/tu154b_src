<?xml version="1.0"?>

<!-- AZS-2; AGD-1, AI left pilot -->

<PropertyList>
 <path>../../shared/switches/azs.ac</path>
 
 <animation>
  <type>pick</type>
  <object-name>clickspot_up</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
	interpolate("an24/AZS/sw0703", 1.0, 0.1 );
	sound.click("azs", 3.3, 0.8, 0.87);
      </script>
    </binding>
  </action>
 </animation>

<animation>
  <type>pick</type>
  <object-name>clickspot_down</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
	interpolate("an24/AZS/sw0703", 0.0, 0.1 );
	sound.click("azs", 3.3, 0.8, 0.87);
      </script>
    </binding>
  </action>
</animation>

 <animation>
   <type>rotate</type>
   <object-name>lever</object-name>
   <property>an24/AZS/sw0703</property>
  <interpolation>
    <entry><ind>0.0</ind>	<dep>-20.0</dep></entry>
    <entry><ind>1.0</ind>	<dep>20.0</dep></entry>
  </interpolation>
  <axis>
    <x>0.0</x>
    <y>-1.0</y>
    <z>0.0</z>
  </axis>
 </animation>

</PropertyList>
