<?xml version="1.0"?>

<!-- Tank-group 1 pump ECN-14 left set to auto(1)/off(0)/manual(-1); (switch 586) -->

<PropertyList>
 <path>../../shared/switches/switch03.ac</path>
 
 <animation>
  <type>pick</type>
  <object-name>clickspot_up</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
        interpolate("an24/FuelControl/sw0402", 1.0, 0.1 );
      </script>
    </binding>
  </action>
   <hovered>
       <binding>
         <command>set-tooltip</command>
         <label>Main Pump %s</label>
         <property>an24/FuelControl/sw0402</property>
         <tooltip-id>mainpumplauto</tooltip-id>
         <mapping>nasal</mapping>
         <script>
	var mainpumplauto = ['OFF', 'Auto', 'Manual'];
	return mainpumplauto[arg[0]];
         </script>
       </binding>
    </hovered>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>clickspot_middle</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
        interpolate("an24/FuelControl/sw0402", 0.0, 0.1 );
      </script>
    </binding>
  </action>
   <hovered>
       <binding>
         <command>set-tooltip</command>
         <label>Main Pump %s</label>
         <property>an24/FuelControl/sw0402</property>
         <tooltip-id>mainpumploff</tooltip-id>
         <mapping>nasal</mapping>
         <script>
	var mainpumploff = ['OFF', 'Auto', 'Manual'];
	return mainpumploff[arg[0]];
         </script>
       </binding>
    </hovered>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>clickspot_down</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
        interpolate("an24/FuelControl/sw0402", -1.0, 0.1 );
      </script>
    </binding>
  </action>
   <hovered>
       <binding>
         <command>set-tooltip</command>
         <label>Main Pump %s</label>
         <property>an24/FuelControl/sw0402</property>
         <tooltip-id>mainpumplman</tooltip-id>
         <mapping>nasal</mapping>
         <script>
	var mainpumplman = ['OFF', 'Auto', 'Manual'];
	return mainpumplman[arg[0]];
         </script>
       </binding>
    </hovered>
</animation>

 <animation>
   <type>rotate</type>
   <object-name>lever</object-name>
   <property>an24/FuelControl/sw0402</property>
  <interpolation>
    <entry><ind>-1.0</ind>	<dep>-35.0</dep></entry>
    <entry><ind>1.0</ind>	<dep>35.0</dep></entry>
  </interpolation>
  <axis>
    <x>0.0</x>
    <y>-1.0</y>
    <z>0.0</z>
  </axis>
 </animation>

</PropertyList>
