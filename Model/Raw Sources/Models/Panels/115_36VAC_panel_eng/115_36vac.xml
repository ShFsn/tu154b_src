<!-- ********** Hatch *********** -->
 <animation>
  <type>pick</type>
  <object-name>fuse_panel_hatch</object-name>
  <visible>true</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
        if( getprop("an24/AC_Panel/fuse_panel_hatch" ) == 1.0 ){
        interpolate("an24/AC_Panel/fuse_panel_hatch", 0.0, 0.2 );
        }
        else {
        interpolate("an24/AC_Panel/fuse_panel_hatch", 1.0, 0.2 );
        }
      </script>
    </binding>
  </action>
 </animation>

 <animation>
   <type>rotate</type>
   <object-name>fuse_panel_hatch</object-name>
   <property>an24/AC_Panel/fuse_panel_hatch</property>
   <factor>140</factor>
  <center>
    <x-m>0.001</x-m>
    <y-m>0.0</y-m>
    <z-m>-0.022</z-m>
  </center>
  <axis>
    <x>0</x>
    <y>1</y>
    <z>0</z>
  </axis>
 </animation>

