<?xml version="1.0"?>

<!-- R-802; Radio frequency dial station -->
<PropertyList>
 <path>r-802sel.ac</path>

 <animation>
  <type>material</type>
  <object-name>lightplate</object-name>
  <object-name>dig100</object-name>
  <object-name>dig10</object-name>
  <object-name>dig1</object-name>
  <emission>
   <red>1.0</red>
   <green>0.7</green>
   <blue>0.5</blue>
   <factor-prop>controls/lighting/overhead_instr</factor-prop>
  </emission>
 </animation>

 <animation>
   <type>knob</type>
   <object-name>dial100</object-name>
   <property>an24/R-802/dial100</property>
   <factor>-20</factor>
   <axis>
    <x>0</x>
    <y>1</y>
    <z>0</z>
   </axis>
   <center>
    <x-m>0.0</x-m>
    <y-m>0.0</y-m>
    <z-m>-0.0557</z-m>
   </center>
    <action>
     <binding>
      <command>property-adjust</command>
      <property>an24/R-802/dial100</property>
      <factor>-10</factor>
      <min>100</min>
      <max>160</max>                
      <wrap>true</wrap>
     </binding>
    </action>
   <hovered>
       <binding>
         <command>set-tooltip</command>
         <label>Freq %3.2fMHz</label>
         <tooltip-id>r802-2-freq-100</tooltip-id>
         <property>an24/R-802/finalfreq</property>
       </binding>
    </hovered>
 </animation>

<animation>
  <type>rotate</type>
  <object-name>dig100</object-name>
  <property>an24/R-802/dial100</property>
  <factor>6.0</factor>
  <axis>
    <x>0</x>
    <y>1</y>
    <z>0</z>
  </axis>
   <center>
    <x-m>-0.0038</x-m>
    <y-m>0.0</y-m>
    <z-m>-0.0365</z-m>
   </center>
</animation>

 <animation>
   <type>knob</type>
   <object-name>dial10</object-name>
   <property>an24/R-802/dial10</property>
   <factor>20</factor>
   <axis>
    <x>0</x>
    <y>1</y>
    <z>0</z>
   </axis>
   <center>
    <x-m>0.0</x-m>
    <y-m>0.0</y-m>
    <z-m>-0.0557</z-m>
   </center>
    <action>
     <binding>
      <command>property-adjust</command>
      <property>an24/R-802/dial10</property>
      <factor>-1</factor>
      <min>0</min>
      <max>10</max>                
      <wrap>true</wrap>
     </binding>
    </action>
   <hovered>
       <binding>
         <command>set-tooltip</command>
         <label>Freq %3.2fMHz</label>
         <tooltip-id>r802-2-freq-10</tooltip-id>
         <property>an24/R-802/finalfreq</property>
       </binding>
    </hovered>
 </animation>

<animation>
  <type>rotate</type>
  <object-name>dig10</object-name>
  <property>an24/R-802/dial10</property>
  <factor>36.0</factor>
  <axis>
    <x>0</x>
    <y>1</y>
    <z>0</z>
  </axis>
   <center>
    <x-m>-0.0038</x-m>
    <y-m>0.0</y-m>
    <z-m>-0.0365</z-m>
   </center>
</animation>

 <animation>
   <type>knob</type>
   <object-name>dial1</object-name>
   <property>an24/R-802/dial1</property>
   <factor>20</factor>
   <axis>
    <x>0</x>
    <y>1</y>
    <z>0</z>
   </axis>
   <center>
    <x-m>0.0</x-m>
    <y-m>0.0</y-m>
    <z-m>-0.0557</z-m>
   </center>
    <action>
     <binding>
      <command>property-adjust</command>
      <property>an24/R-802/dial1</property>
      <factor>-0.5</factor>
      <min>0</min>
      <max>10</max>                
      <wrap>true</wrap>
     </binding>
    </action>
   <hovered>
       <binding>
         <command>set-tooltip</command>
         <label>Freq %3.2fMHz</label>
         <tooltip-id>r802-2-freq-1</tooltip-id>
         <property>an24/R-802/finalfreq</property>
       </binding>
    </hovered>
 </animation>

<animation>
  <type>rotate</type>
  <object-name>dig1</object-name>
  <property>an24/R-802/dial1</property>
  <factor>36.0</factor>
  <axis>
    <x>0</x>
    <y>1</y>
    <z>0</z>
  </axis>
   <center>
    <x-m>-0.0038</x-m>
    <y-m>0.0</y-m>
    <z-m>-0.0365</z-m>
   </center>
</animation>

<animation>
  <type>pick</type>
  <object-name>dialscrew</object-name>
  <visible>true</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
        interpolate("an24/R-802/dialscrew", 1.0, 0.1 );
      </script>
    </binding>
      <mod-up>
       <binding>
	<command>nasal</command>
	<script>
	interpolate("an24/R-802/dialscrew", 0.0, 0.1 );
        </script>
       </binding>
      </mod-up>
  </action>
   <hovered>
       <binding>
         <command>set-tooltip</command>
         <label>Send to Memory
    Channel %u</label>
         <tooltip-id>r802-2-freq</tooltip-id>
         <property>an24/R-802/channel</property>
       </binding>
    </hovered>
</animation>

<animation>
  <type>translate</type>
  <object-name>dialscrew</object-name>
  <property>an24/R-802/dialscrew</property>
  <factor>0.0016</factor>
  <axis>
    <x>-1</x>
    <y>0</y>
    <z>0</z>
  </axis>
</animation>

 <animation>
   <type>knob</type>
   <object-name>r-802sel_volume</object-name>
   <property>an24/R-802/kn_volume-2</property>
   <factor>-90</factor>
   <axis>
    <x>1</x>
    <y>0</y>
    <z>0</z>
   </axis>
   <center>
    <x-m>0.0</x-m>
    <y-m>0.0127</y-m>
    <z-m>0.0282</z-m>
   </center>
    <action>
     <binding>
      <command>property-adjust</command>
      <property>an24/R-802/kn_volume-2</property>
      <factor>-0.1</factor>
      <min>0</min>
      <max>1</max>                
      <wrap>false</wrap>
     </binding>
    </action>
   <hovered>
       <binding>
         <command>set-tooltip</command>
         <label>Volume: %3d%%</label>
         <measure-text>Volume: 100%</measure-text>
         <tooltip-id>r802-2-vol</tooltip-id>
         <mapping>percent</mapping>
         <property>an24/R-802/kn_volume-2</property>
       </binding>
     </hovered>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>clickspot_up</object-name>
  <visible>false</visible>
  <!--action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
        interpolate("an24/R-802/sw0403", 1.0, 0.1 );
      </script>
    </binding>
  </action-->
</animation>

 <animation>
  <object-name>clickspot_middle</object-name>
  <type>pick</type>
  <visible>false</visible>
</animation>

 <animation>
  <type>pick</type>
  <object-name>clickspot_down</object-name>
  <visible>false</visible>
  <!--action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
        interpolate("an24/R-802/sw0403", 0.0, 0.1 );
      </script>
    </binding>
  </action-->
</animation>

<!-- TEXT OSG -->
  <animation>
   <type>select</type>
   <object-name>text802sel</object-name>
    <condition>
     <or>
      <property>sim/panel-hotspots</property>
      <property>sim/panel-hotspots_comm</property>
     </or>
    </condition>
  </animation>

  <text>
    <name>text802sel</name>
    <type type="string">literal</type>
    <text type="string">R-802 Comm Device No.2</text>
    <draw-text type="bool">true</draw-text>
    <font>Helvetica.txf</font>
    <character-size type="double">0.014</character-size>
    <character-aspect-ratio type="double">1.0</character-aspect-ratio>
    <axis-alignment>xz-plane</axis-alignment>
    <max-height>0.05</max-height>
    <max-width>0.05</max-width>
    <font-resolution>
      <width type="int">64</width>
      <height type="int">64</height>
    </font-resolution>
    <alignment>center-center</alignment>
    <offsets>
     <x-m>0.025</x-m>
     <y-m>0.000</y-m>
     <z-m>0.010</z-m>
     <heading-deg>90</heading-deg>
    </offsets>
  </text>
  <animation>
    <type>material</type>
    <object-name>text802sel</object-name>
    <emission>
      <red>1.0</red>
      <green>1.0</green>
      <blue>1.0</blue>
    </emission>
  </animation>

</PropertyList>
