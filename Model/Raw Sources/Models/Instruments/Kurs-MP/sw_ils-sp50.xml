<?xml version="1.0"?>

<!-- Switch between regimes SP-50 and ILS -->

<PropertyList>
 <path>../../shared/switches/2vg-15k.ac</path>

 <animation>
  <type>pick</type>
  <object-name>clickspot_up</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
        interpolate("an24/Kurs-MP/sp-50_ILS", 1.0, 0.1 );
      </script>
    </binding>
  </action>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>clickspot_down</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
        interpolate("an24/Kurs-MP/sp-50_ILS", -1.0, 0.1 );
      </script>
    </binding>
  </action>
 </animation>

 <animation>
   <type>rotate</type>
   <object-name>lever</object-name>
   <property>an24/Kurs-MP/sp-50_ILS</property>
  <interpolation>
    <entry><ind>1.0</ind>	<dep>20.0</dep></entry>
    <entry><ind>-1.0</ind>	<dep>-20.0</dep></entry>
  </interpolation>
  <axis>
    <x>0.0</x>
    <y>-1.0</y>
    <z>0.0</z>
  </axis>
 </animation>
</PropertyList>
