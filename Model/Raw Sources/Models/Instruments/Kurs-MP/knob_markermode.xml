<?xml version="1.0"?>

<!-- Knob to select marker modes -->

<PropertyList>
 <path>../../shared/knobs_buttons/vs-33.ac</path>

 <animation>
   <type>knob</type>
   <object-name>11p1n</object-name>
   <property>an24/Kurs-MP/markermode</property>
   <factor>-40</factor>
   <offset-deg>40</offset-deg>
  <axis>
    <x>1.0</x>
    <y>0.0</y>
    <z>0.0</z>
  </axis>
    <action>
     <binding>
      <command>property-adjust</command>
      <property>an24/Kurs-MP/markermode</property>
      <factor>-1</factor>
      <min>0</min>
      <max>2</max>                
      <wrap>false</wrap>
     </binding>
    </action>
 </animation>

</PropertyList>
