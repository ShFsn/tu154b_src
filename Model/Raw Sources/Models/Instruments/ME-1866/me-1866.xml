<!-- ITG-2, left engine EGT -->

<PropertyList>
 <path>me-1866.ac</path>

 <animation>
   <type>material</type>
   <object-name>me_housing</object-name>
   <!--object-name>me_cover</object-name-->
   <emission>
     <red>0.7</red>
     <green>0.3</green>
     <blue>0.3</blue>
     <factor-prop>controls/lighting/console-l_red</factor-prop>
   </emission>
 </animation>

 <animation>
   <type>material</type>
   <object-name>me_face</object-name>
   <object-name>me_needle</object-name>
   <emission>
     <red>1.0</red>
     <green>0.6</green>
     <blue>0.6</blue>
     <factor-prop>controls/lighting/console-l_red</factor-prop>
   </emission>
 </animation>

