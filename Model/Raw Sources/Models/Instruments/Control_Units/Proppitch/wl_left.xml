<?xml version="1.0"?>

<!-- Left prop pitch-lock -->

<PropertyList>
 <path>../../../shared/lamps/sltsn.ac</path>

<animation>
 <type>material</type>
 <object-name>holder</object-name>
 <object-name>lampon</object-name>
 <texture>../../../shared/lamps/sltsnred.png</texture>
</animation>

<animation>
 <type>select</type>
 <condition>
  <and>
   <greater-than>
    <property>an24/Electrics/AZSemerg_V</property>
    <value>20.0</value>
   </greater-than>
   <less-than>
    <property>fdm/jsbsim/fcs/prop-governor/min-blade-angle[0]</property>
    <value>19</value>
   </less-than>
  </and>
 </condition>
 <object-name>lampon</object-name>
</animation>

</PropertyList>
