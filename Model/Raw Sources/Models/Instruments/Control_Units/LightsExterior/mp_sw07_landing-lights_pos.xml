<?xml version="1.0"?>

<!-- landing lights PRF-4/88; Extend/Retract; Item 364 -->
<PropertyList>
 <path>../../../shared/switches/2ppng-15k.ac</path>

 <animation>
  <type>pick</type>
  <object-name>clickspot_up</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
	interpolate("an24/lights/sw_PRF-4motor", 1.0, 0.1 );
      </script>
    </binding>
  </action>
   <hovered>
    <binding>
      <command>set-tooltip</command>
      <tooltip-id>PRFdown</tooltip-id>
      <label>     PRF-4
Landing Lights
    EXTEND</label>
    </binding>
   </hovered>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>clickspot_middle</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
	interpolate("an24/lights/sw_PRF-4motor", 0.0, 0.1 );
      </script>
    </binding>
  </action>
   <hovered>
    <binding>
      <command>set-tooltip</command>
      <tooltip-id>PRFstop</tooltip-id>
      <label>     PRF-4
Landing Lights
    STOP</label>
    </binding>
   </hovered>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>clickspot_down</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
	interpolate("an24/lights/sw_PRF-4motor", -1.0, 0.1 );
      </script>
    </binding>
  </action>
   <hovered>
    <binding>
      <command>set-tooltip</command>
      <tooltip-id>PRFup</tooltip-id>
      <label>     PRF-4
Landing Lights
   RETRACT</label>
    </binding>
   </hovered>
 </animation>

 <animation>
   <type>rotate</type>
   <object-name>lever</object-name>
   <property>an24/lights/sw_PRF-4motor</property>
  <interpolation>
    <entry><ind>-1.0</ind>	<dep>-20.0</dep></entry>
    <entry><ind>1.0</ind>	<dep>20.0</dep></entry>
  </interpolation>
  <axis>
    <x>0.0</x>
    <y>-1.0</y>
    <z>0.0</z>
  </axis>
 </animation>
 
</PropertyList>
