<?xml version="1.0"?>

<!-- Test button 8981 of "Doors and Hatches open" panel on back of overhead -->
<PropertyList>
 <path>../../../shared/knobs_buttons/5k.ac</path><!--5ks-->

 <animation>
  <type>pick</type>
  <object-name>press_me</object-name>
  <visible>true</visible>
   <action>
    <button>0</button>
    <repeatable>true</repeatable>
  <binding>
   <command>nasal</command>
    <script>
	interpolate("an24/doorshatches/btn_test", 1.0, 0.1 );
	interpolate("an24/doorshatches/test", 1.0, 0.1 );
    </script>
  </binding>
 <mod-up>
  <binding>
   <command>nasal</command>
    <script>
	interpolate("an24/doorshatches/btn_test", 0.0, 0.1 );
	interpolate("an24/doorshatches/test", 0.0, 0.1 );
    </script>
  </binding>
 </mod-up>
   </action>

   <hovered>
       <binding>
         <command>set-tooltip</command>
         <label>Doors/hatches Open
  Lights Control</label>
         <tooltip-id>doorstestbtn</tooltip-id>
       </binding>
     </hovered>
 </animation>

 <animation>
  <type>translate</type>
  <object-name>press_me</object-name>
  <property>an24/doorshatches/btn_test</property>
  <factor>0.005</factor>
  <axis>
    <x>0</x>
    <y>0</y>
    <z>-1</z>
  </axis>
 </animation>

</PropertyList>
